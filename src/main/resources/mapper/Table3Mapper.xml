<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cbrc.back.mapper.Table3Mapper">







    <insert id="insert" parameterType="com.cbrc.back.model.Table3">
        insert into Table3(col1,col2,col3,col4,col5,col6,col7,col8,col9,col10,col11,userid)  values (#{col1},#{col2},#{col3},#{col4},#{col5},#{col6},#{col7},#{col8},#{col9},#{col10},#{col11},#{userid})
    </insert>



    <select id="findByOrgName" parameterType="java.lang.String" resultType="com.cbrc.back.model.Table3">
        select * from Table3 where orgName= #{orgName} and date &gt;= #{fromData} and date  &lt;=  #{endData} and filetype = #{filetype}
    </select>






    <select id="find"  resultType="com.cbrc.back.model.Table3">
        select * from Table3
    </select>



    <select id="findById" parameterType="java.lang.String" resultType="com.cbrc.back.model.Table3">
        select * from Table3 where id= #{id}
    </select>



</mapper>